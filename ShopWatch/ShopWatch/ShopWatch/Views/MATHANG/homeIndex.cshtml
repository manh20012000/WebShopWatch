        .product {
            margin-top: 20px;
            overflow: hidden;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
        }

            .product img {
                margin-bottom: 20px;
                width: auto;
                height: auto;
                transition: transform 0.2s ease
            }

            .product:hover img {
                transform: scale(1.4);
            }

        .top_nav {
            background: #8B0000;
        }

        .banner_category a {
            display: inline-block;
            position: relative;
            overflow: hidden;
            transition: transform 0.3s ease; /* Hiệu ứng chuyển động */
            will-change: transform;
        }

            .banner_category a:hover {
                transform: scale(1.1); /* Phóng to ảnh lên 110% khi hover */
            }
    </style>


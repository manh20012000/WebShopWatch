@model ShopWatch.Models.DIADIEM

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


}<div style="margin:200px 10px 10px 10px">


    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            <h4> THÊM ĐỊA ĐIỂM GIAO HÀNG MỚI </h4>
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div class="form-group">
                <input name="MAKHACHHANG" value="@ViewBag.KHANHHANG.MAKHACHHANG" type="hidden" />
                @Html.LabelFor(model => model.MAKHACHHANG, "TENKHACHHANG", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    <label class="form-control">@ViewBag.KHANHHANG.TENKHACHHANG </label>
                   
                    @Html.ValidationMessageFor(model => model.KHACHHANG.TENKHACHHANG, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.TENDIACHI, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.TENDIACHI, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.TENDIACHI, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.SDT, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.SDT, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.SDT, "", new { @class = "text-danger" })
                </div>
            </div>



            <div class="form-group">
                <div class="col-md-offset-2 col-md-10 ">
                    <input type="submit" value="Create" class="btn btn-default form-control" style="background-color:burlywood" />
                </div>
            </div>
        </div>
    }
</div>
